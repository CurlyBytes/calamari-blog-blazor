{
  "Serilog": {
    "WriteTo": [
      "Console",
      "DiagnosticTrace"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    }
  },

  "AllowedHosts": "*",

  "Squidex": {
    "url": "https://cloud.squidex.io",
    "appName": "calamari-blog",
    "clientId": "calamari-blog:website",
    "clientSecret": "client-secret",
    "webhookAssetSecret": "asset-secret",
    "webhookContentSecret": "content-secret"
  },

  "Cache": {
    "ExpiryTimeMinutes": 60,
    "IsEnabled": true
  },

  "Email": {
    "IsEnabled": false,
    "SendGrid": {
      "APIKey": "insert-api-key-here"
    },
    "SystemEmailAddress": "system@example.com",
    "SystemEmailName": "Calamari Blog",
    "RecipientEmailAddress": "author@example.com"
  },

  "Disqus": {
    "EmbedJsUrl": "https://some-cool-website.disqus.com/embed.js"
  }
}
